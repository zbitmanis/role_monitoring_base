route:
  group_by: ['alertname']
  group_wait: 30s
  group_interval: 5m
  repeat_interval: 1h
  receiver: dummy
receivers:
  - name: dummy
{% for receiver in alertmanager_recivers %}

  - name: "receiver.name"
    receiver.config 
 {% endfor %}
  
inhibit_rules:
  - source_match:
      severity: 'critical'
    target_match:
      severity: 'warning'
    equal: ['alertname', 'dev', 'instance']
